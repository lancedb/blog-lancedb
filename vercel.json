{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "hugo --gc -b https://$VERCEL_PROJECT_PRODUCTION_URL",
  "build": {
    "env": {
      "HUGO_VERSION": "0.147.7"
    }
  },
  "headers": [
    {
      "source": "/fonts/(.*).woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
} 